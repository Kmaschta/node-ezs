{
  "name": "ezs",
  "version": "5.1.1",
  "description": "A wrapper to build Stream transformers with functional style",
  "directories": {
    "test": "test"
  },
  "main": "./lib/index.js",
  "bin": {
    "ezs": "./bin/ezs"
  },
  "scripts": {
    "test": "istanbul test ./node_modules/.bin/_mocha -- -R spec",
    "lint": "eslint --ext=.js ./test/*.js ./src/*.js",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build",
    "pretest": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/touv/node-ezs.git"
  },
  "keywords": [
    "stream",
    "transformer"
  ],
  "author": "Nicolas Thouvenin <nthouvenin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/touv/node-ezs/issues"
  },
  "homepage": "https://github.com/touv/node-ezs#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "JSONStream": "^1.3.2",
    "autocast": "0.0.4",
    "deep-object-diff": "^1.0.4",
    "filtrex": "^0.5.4",
    "lodash": "^4.17.4",
    "memored": "^1.1.1",
    "merge-stream": "^1.0.1",
    "notevil": "^1.1.0",
    "p-map": "^1.2.0",
    "yargs": "^11.0.0"
  }
}
